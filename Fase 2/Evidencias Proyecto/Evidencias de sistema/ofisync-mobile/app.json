{
  "expo": {
    "name": "ofisync-mobile",
    "slug": "ofisync-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoOfisync.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/LogoOfisync.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoOfisync.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alexpulgar.ofisyncmobile"
    },
    "web": {
      "favicon": "./assets/LogoOfisync.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "65200cb0-e79f-4f87-8f2c-ace1310975f0"
      }
    }
  }
}
